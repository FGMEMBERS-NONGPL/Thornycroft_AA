<?xml version="1.0"?>
<PropertyList>

	<path>ThornyAA.ac</path>

	<offsets>
		<x-m>0.0</x-m>
		<y-m>0.0</y-m>
		<z-m>0.0</z-m>
		<heading-deg>0.0</heading-deg>
	</offsets>

<!-- AA gun smoke/flame -->
  <model>
    <path>Aircraft/Thornycroft_AA/Models/Effects/guns/fire.xml</path>
    <offsets>
      <x-m> -0.10 </x-m>
      <y-m> 0.00  </y-m>
      <z-m> 3.163 </z-m>
      <roll-deg>    0.0 </roll-deg>
      <pitch-deg>   0.0 </pitch-deg>
      <heading-deg> 0.0 </heading-deg>
    </offsets>
  </model>
	

<!-- speedometer -->
  <model>
    <path>Aircraft/Thornycroft_AA/Models/Instruments/Speedometer/speedometer.xml</path>
    <offsets>
      <x-m> -2.349 </x-m>
      <y-m> 0.302 </y-m>
      <z-m> 2.063 </z-m>
      <roll-deg>    0.0 </roll-deg>
      <pitch-deg>   -19.0 </pitch-deg>
      <heading-deg> 0.0 </heading-deg>
    </offsets>
  </model>


<!-- gas gauge -->
  <model>
    <path>Aircraft/Thornycroft_AA/Models/Instruments/Fuel/fuelgauge.xml</path>
    <offsets>
      <x-m> -2.349 </x-m>
      <y-m> 0.104 </y-m>
      <z-m> 2.063 </z-m>
      <roll-deg>    0.0 </roll-deg>
      <pitch-deg>   -19.0 </pitch-deg>
      <heading-deg> 0.0 </heading-deg>
    </offsets>
  </model>

<!-- controls, wheel and suspension animations -->

<!-- gear selector anim. -->

 <animation>
  <object-name>gearlever</object-name>
  <type>rotate</type>
  <property>controls/gearstick-y</property>
  <factor>-1</factor>
  <center>
   <x-m>-2.096</x-m>
   <y-m> 0.7602</y-m>
   <z-m>1.305</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
  <object-name>gearlever</object-name>
  <type>rotate</type>
  <property>controls/gearstick-x</property>
  <factor>-1</factor>
  <center>
   <x-m>-2.096</x-m>
   <y-m> 0.7602</y-m>
   <z-m>1.305</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
	

 <!-- handbrake -->
 <animation>
  <object-name>parkingbrake</object-name>
  <type>rotate</type>
  <property>controls/gear/brake-parking</property>
  <factor>20</factor>
  <center>
   <x-m>-2.0935</x-m>
   <y-m> 0.7064</y-m>
   <z-m>1.313</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<!-- steering -->
  <animation>
    <type>rotate</type>
    <object-name>steeringwheel</object-name>
    <property>controls/flight/aileron</property>
    <factor>-450</factor>
  <axis>
   <x1-m>-2.40285</x1-m>
   <y1-m>0.3637</y1-m>
   <z1-m>1.67013</z1-m>
   <x2-m>-2.0052</x2-m>
   <y2-m>0.3641</y2-m>
   <z2-m>2.14737 </z2-m>
  </axis>
  </animation>

<!-- Front Axle -->
 <animation>
  <type>translate</type>
  <object-name>L-frontwheel</object-name>
  <object-name>R-frontwheel</object-name>
  <object-name>frontaxle</object-name>
  <property>gear/gear[0]/compression-norm</property>
    <condition>
        <greater-than-equals>
          <property>gear/gear[1]/compression-norm</property>
          <property>gear/gear[0]/compression-norm</property>
        </greater-than-equals>
    </condition>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>-0.025</dep>
   </entry>
   <entry>
    <ind>1.00</ind>
    <dep>0.155</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

